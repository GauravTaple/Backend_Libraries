    <style type="text/css">
        @page {
            size: A4;
            margin-left: 50px;
            margin-right: 50px;
            margin-top: 30mm;
            margin-bottom: 24mm;

            @top-left {
                content: element(header);
            }

            @bottom-left {
                content: element(footer);
            }
        }

        body {
            font-family: EmbCourierNew;
            font-size: 11pt;
            text-align: justify;
            line-height: 1.5;
        }

        header {
            font-family: EmbArial;
            text-align: center;
            position: running(header);
            width: 677px;
            margin-left: 7.5px;
            font-size: 6.5pt;
        }

        footer {
            font-family: EmbArial;
            font-size: 8pt;
            position: running(footer);
        }

        .leftPadding {
            padding-left: 6px;
        }

        .fontBold {
            font-family: EmbCourierNewBold;
            font-weight: normal;
        }

        .break {
            word-wrap: break-word;
            white-space: normal;
            overflow-wrap: break-word;
        }

        .page-break {
            page-break-before: always;
        }

        table {
            width: 100%;
            border-collapse: collapse;
        }

        .paddingTopDiv {
            padding-top: 20px;
        }

        .footerSvgImages {
            vertical-align: middle;
            margin-right: 5px;
            display: inline-block;
        }

        .infoTable td {
            vertical-align: top;
        }

        .customerInfo {
            width: 60%;
            height: 120px;
            max-height: 120px;
        }

        .alignCenter {
            text-align: center;
        }

        .dividerLine {
            border: 1px solid;
            margin: 15px 0px;
        }

        .bottomMsg {
            font-size: 10pt;
            font-family: EmbCenturyGothicBold;
            font-weight: normal;
        }

        .bankIslamLogo{
            vertical-align: middle; 
            margin-left: 5px; 
            width:270px; 
            height:65px;
            display: inline-block;
            margin-bottom: 7px;
        }
    </style>
</head>
